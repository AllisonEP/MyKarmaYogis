<h1><%=@project.title%></h1>
<h2>Creator: <%=@project.volunteer.username%></h2>

<div><%= @project.content%></div>

<p><%= link_to 'See Collaberating Teams', project_teams_path(@project)%></p>

<p><%= link_to 'Add a Collaberating Team', new_project_team_path(@project)%></p>

<h4>Teams:</h4>
<% @project.teams.each do |t|%>
<li><strong><%= t.volunteer.username%></strong> says "<%= link_to t.content, team_path(t)%>" </li>
<% end %>